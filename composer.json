{
    "name": "mmeyer2k/dcrypt",
    "description": "A petite library of encryption functionality for PHP",
    "keywords": ["encryption", "hashing", "pbkdf", "aes", "otp", "pkcs7", "rc4", "spritz", "mcrypt", "openssl"],
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Meyer",
            "email": "m.meyer2k@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "mmeyer2k/retro": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Dcrypt\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dcrypt\\Support\\": "helpers/"
        }
    },
    "minimum-stability": "dev"
}
